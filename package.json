		"d3-quadtree": "^3.0.1",
		"d3-scale": "^4.0.2",
		"d3-selection": "^3.0.0",
		"d3-shape": "^3.2.0",
		"d3-time-format": "^4.1.0",
		"d3-transition": "^3.0.1",
		"luxon": "^3.6.1",
		"papaparse": "^5.5.3"
	}
}
